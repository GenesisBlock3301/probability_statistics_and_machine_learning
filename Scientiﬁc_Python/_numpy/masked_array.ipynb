    "import numpy as np\n",
    "from numpy import ma\n",
    "\n",
    "x = np.arange(10)\n",
    "x[0] = 4444\n",
    "y = ma.masked_array(x, x<5)\n",
    "print(y)\n",
    "print()\n",
    "\n",
    "print(y)"
   ],
